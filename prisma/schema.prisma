// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}
model Pegawai {
  bil    Int     @id @default(autoincrement())
  nama  String  
  statusPegawai  String
  mdcNumber Int
}
model Juruterapi {
  bil    Int     @id @default(autoincrement())
  nama  String  
  statusPegawai  String
  mdtbNumber String
}
model Fasiliti {
  bil    Int     @id @default(autoincrement())
  nama  String  
  negeri  String
  daerah  String
  kodFasiliti  String
  kodFasilitiGiret  String
}
model Kkiakd {
  bil    Int     @id @default(autoincrement())
  nama  String
  namaHospital String
  negeri  String
  daerah  String
  kodFasiliti  String
  jenisFasiliti String
}
model Account {
  id                 String  @id @default(cuid())
  userId             String
  type               String
  provider           String
  providerAccountId  String
  refresh_token      String?  
  access_token       String?  
  expires_at         Int?
  token_type         String?
  scope              String?
  id_token           String?
  session_state      String?

  user User @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([provider, providerAccountId])
}
model Session {
  id           String   @id @default(cuid())
  sessionToken String   @unique
  userId       String
  expires      DateTime
  user         User     @relation(fields: [userId], references: [id], onDelete: Cascade)
}
model User {
  id            String    @id @default(cuid())
  name          String?
  username      String?   @unique
  password      String?
  email         String?   @unique
  emailVerified DateTime?
  image         String?
  accounts      Account[]
  sessions      Session[]
}
model VerificationToken {
  identifier String
  token      String   @unique
  expires    DateTime

  @@unique([identifier, token])
}